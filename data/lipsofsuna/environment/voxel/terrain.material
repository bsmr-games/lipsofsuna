material terrainmaterial
{
	set $cullmode "clockwise"
	set $texture1 "granite1.png"
	set $texture2 "granite1n.png"
	set $texture3 "stone-001.dds"
	technique
	{
		pass LOS1 : terrainmaterial/3/lighting
		{
		}
	}
	technique
	{
		scheme quality3
		pass LOS1 : terrainmaterial/3/lighting
		{
		}
	}
	technique
	{
		scheme quality2outline
		pass LOS1 : terrainmaterial/2/lighting
		{
		}
	}
	technique
	{
		scheme quality2
		pass LOS1 : terrainmaterial/2/lighting
		{
		}
	}
	technique
	{
		scheme quality1
		pass
		{
			ambient 1.0 1.0 1.0
			diffuse vertexcolour
			specular 0.1 0.1 0.1 0.1 64
			texture_unit
			{
				texture $texture1
			}
		}
	}
}

