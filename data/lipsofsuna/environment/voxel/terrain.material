pass terrainmaterial/3/lighting
{
	set $cullmode "clockwise"
	set $texture1 "granite1.png"
	set $texture2 "granite1n.png"
	set $texture3 "stone-001.dds"
	max_lights 3
	ambient 0.7 0.7 0.7
	diffuse vertexcolour
	specular 0.1 0.1 0.1 0.1 64
	cull_hardware $cullmode
	texture_unit
	{
		texture $texture1
	}
	texture_unit
	{
		texture $texture2
	}
	texture_unit
	{
		texture $texture3
	}
	texture_unit
	{
		texture celramp1.png 1d
		tex_address_mode clamp
	}
	texture_unit
	{
		texture celramp2.png 1d
		tex_address_mode clamp
	}
	texture_unit
	{
		content_type shadow
	}
	vertex_program_ref terrain1_vert
	{
	}
	fragment_program_ref terrain1_frag
	{
	}
}

